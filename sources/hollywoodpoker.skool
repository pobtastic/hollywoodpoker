 $78E7 RST $10        ;
 $78E8 LD A,$07       ;
 $78EA RST $10        ; }
 $78EB LD A,$11       ; {Set PAPER: #INK$00.
 $78ED RST $10        ;
 $78EE XOR A          ;
 $78EF RST $10        ; }
 $78F0 POP HL         ; Restore #REGhl from the stack.
 $78F1 PUSH HL        ; Stash #REGhl on the stack.
 $78F2 LD A,(HL)      ; #REGa=*#REGhl.
 $78F3 CP $0D         ; {Jump to #R$7907 if the key is "enter".
